
<view  class="cu-chat">
  <view  class="cu-item">
    <view class="cu-avatar round" style="background-image:url({{IconAI}})"></view>    
    <view class="main">
      <view class="content bg-gradual-green shadow">
        
        <view class="flex flex-direction justify-start">  
          <view class="text-sm">
            {{item.msg.eDate}}-
            {{item.msg.eTime}}
            <view class="round" style="position:absolute;top:5px;right:5px;width:30px;height:30px;background-image:url({{baseURL}}/img/download/{{item.msg.imgHash}})"/>
          </view>
          <view class="margin-td">
            <text class="text-xxl">{{item.msg.value}}</text>
            <text class="text-sm">{{item.msg.unit}}</text>
            <text class="text-sm text-cut">{{item.msg.stuff}}</text>
          </view>
        </view>
        
      </view>
      <view class="flex-sub cu-tag margin-sm bg-green">饮食</view>
    </view>
      
    
    <view class="date"> {{item.dateTime}}</view>
  </view>
</view>